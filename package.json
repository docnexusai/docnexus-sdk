{
  "name": "@docnexus/api-client",
  "version": "1.1.0",
  "description": "DocNexus API client and API Platform SDK for docnexus-link and advanced-search (API key platform)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "prepublishOnly": "npm run build",
    "test": "node scripts/test-sdk.mjs"
  },
  "keywords": [
    "docnexus",
    "api",
    "healthcare",
    "kong",
    "api-platform",
    "sdk",
    "docnexus-link",
    "advanced-search"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/docnexus/docnexus-sdk.git"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
